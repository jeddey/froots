{
  "containerDefinitions": [
    {
      "environment": [
        {
          "name": "MYSQL_DATABASE",
          "value": "${MYSQL_DATABASE}"
        },
        {
          "name": "MYSQL_USER",
          "value": "${MYSQL_USER}"
        },
        {
          "name": "MYSQL_PASSWORD",
          "value": "${MYSQL_PASSWORD}"
        },
        {
          "name": "MYSQL_ROOT_PASSWORD",
          "value": "${MYSQL_ROOT_PASSWORD}"
        }
      ],
      "essential": true,
      "image": "mysql:5.7",
      "mountPoints": [
        {
          "containerPath": "/var/lib/mysql",
          "sourceVolume": "Mysql_Data"
        }
      ],
      "name": "mysql",
      "portMappings": [
        {
          "containerPort": 3306,
          "hostPort": 3307
        }
      ]
    },
    {
      "essential": true,
      "image": "nginx:latest",
      "mountPoints": [
        {
          "containerPath": "/var/www/html",
          "sourceVolume": "_"
        },
        {
          "containerPath": "/etc/nginx/conf.d",
          "sourceVolume": "_DockerNginxConf_D"
        }
      ],
      "name": "nginx",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    },
    {
      "environment": [
        {
          "name": "PHP_FPM_ENABLE",
          "value": "1"
        },
        {
          "name": "PHP_FPM_SERVER_ADDR",
          "value": "php"
        },
        {
          "name": "PHP_FPM_SERVER_PORT",
          "value": "9000"
        }
      ],
      "essential": true,
      "image": "339713191041.dkr.ecr.eu-north-1.amazonaws.com/php:latest",
      "links": [
        "mysql",
        "nginx"
      ],
      "mountPoints": [
        {
          "containerPath": "/var/www/html",
          "sourceVolume": "_"
        }
      ],
      "name": "php-fpm",
      "workingDirectory": "/var/www/html"
    }
  ],
  "family": "",
  "volumes": [
    {
      "host": {
        "sourcePath": "./"
      },
      "name": "_"
    },
    {
      "host": {
        "sourcePath": "./docker/nginx/conf.d"
      },
      "name": "_DockerNginxConf_D"
    },
    {
      "host": {
        "sourcePath": "mysql_data"
      },
      "name": "Mysql_Data"
    }
  ]
}
